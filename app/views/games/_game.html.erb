
<% games.each do |game| %>
<tr>
	<td style="text-align:left"><%= game.team_name %></td>
	<td style="text-align:left"><%= game.date %></td>
	<td><%= game.hits %></td>		
	<td><%= game.at_bats %></td>
	<td><%= game.runs %></td>
	<td><%= game.singles %></td>
	<td><%= game.doubles %></td>
	<td><%= game.triples %></td>
	<td><%= game.homeruns %></td>
	<td><%= game.rbis %></td>
	<td><%= game.walks %></td>
	<td><%= game.strike_outs %></td>
	<td></td>
</tr>
<% end %>